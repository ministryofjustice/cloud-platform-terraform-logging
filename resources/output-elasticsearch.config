[OUTPUT]
    Name            es
    Match           kubernetes.*
    Host            ${elasticsearch_host}
    Port            443
    Type            _doc
    Time_Key        @timestamp
    Logstash_Prefix kubernetes_cluster
    tls             On
    Logstash_Format On
    Replace_Dots    On
    Retry_Limit     False
[OUTPUT]
    Name            es
    Match           kubernetes-json.*
    Host            ${elasticsearch_host}
    Port            443
    Type            _doc
    Time_Key        @timestamp
    Logstash_Prefix kubernetes_json
    tls             On
    Logstash_Format On
    Replace_Dots    On
    Retry_Limit     False
[OUTPUT]
    Name            es
