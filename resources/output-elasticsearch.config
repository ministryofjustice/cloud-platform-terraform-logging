[OUTPUT]
    Name            es
    Match           kubernetes.*
    Host            ${elasticsearch_host}
    Port            443
    Type            _doc
    Time_Key        @timestamp
    Logstash_Prefix ${cluster}_kubernetes_cluster
    tls             On
    Logstash_Format On
    Replace_Dots    On
    Retry_Limit     False
